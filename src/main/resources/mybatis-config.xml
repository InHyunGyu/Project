  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<typeAliases>
		<typeAlias type="com.debugking.www.dto.MemberInfo" alias="Member" />
		<typeAlias type="com.debugking.www.dto.Pointslog" alias="Pointlog" />
		<typeAlias type="com.debugking.www.dto.Posts" alias="Post" />
